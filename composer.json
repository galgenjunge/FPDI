{
  "name": "galgenjunge/fpdi",
  "version": "dev-master",
  "description": "FPDI with psr0 and namespaces",
  "type": "library",
  "keywords": ["pdf", "fpdi", "fpdf"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jan Slabon",
      "email": "jan.slabon@setasign.com",
      "homepage": "https://www.setasign.com"
    }
  ],
  "require": {
     "php": ">=5.3.0"
  },
  "autoload": {
    "psr-0": {
